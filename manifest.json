{
  "manifest_version": 2,

  "name": "Traintimes",
  "description": "This extension pulls train departure data from resrobot.se",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://api.trafiklab.se/"
  ]
}
